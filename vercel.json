{
  "version": 2,
  "name": "ai-video-generator",
  "buildCommand": "npx vite build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}